    <!-- StorytellerDef -->
    <StorytellerDef Name="WNA_StorytellerBase" Abstract="True">
        <populationIntentFactorFromPopCurve>
            <points>
                <li>0,8.0</li>
                <li>1,2.0</li>
                <li>4,1.0</li>
                <li>7,0.35</li>
                <li>11,0.0</li>
                <li>20,-1.0</li>
            </points>
        </populationIntentFactorFromPopCurve>
        <populationIntentFactorFromPopAdaptDaysCurve>
            <points>
                <li>(0,0)</li>
                <li>(8,1)</li>
            </points>
        </populationIntentFactorFromPopAdaptDaysCurve>
        <pointsFactorFromDaysPassed>
            <points>
                <li>(10,0.70)</li>
                <li>(40,1.00)</li>
            </points>
        </pointsFactorFromDaysPassed>
        <adaptDaysMin>-60</adaptDaysMin>
        <adaptDaysMax>100</adaptDaysMax>
        <adaptDaysGameStartGraceDays>30</adaptDaysGameStartGraceDays> <!-- only applies to adaptDays past 0 -->
        <adaptDaysLossFromColonistLostByPostPopulation>
            <points>
                <li>(0,30)</li> <!-- 30 days at normal pops; lower at really high pop -->
                <li>(11,30)</li>
                <li>(20,20)</li>
            </points>
        </adaptDaysLossFromColonistLostByPostPopulation>
        <adaptDaysLossFromColonistViolentlyDownedByPopulation>
            <points>
                <li>(1,8)</li>
                <li>(2,6)</li>
                <li>(3,6)</li>
                <li>(11,5)</li>
                <li>(20,3.5)</li>
            </points>
        </adaptDaysLossFromColonistViolentlyDownedByPopulation>
        <adaptDaysGrowthRateCurve>
            <points>
                <li>(-30.01,6)</li>     <!-- under: 8.00 -->
                <li>(-30,3)</li>     <!-- -30: 3.00 -->
                <li>(-0.01,3)</li>
                <li>(0,1)</li>     <!-- 0: 1.00 -->
                <li>(59.99,1)</li>
                <li>(60,0.5)</li> <!-- 60: 0.50 -->
                <li>(119.99,0.5)</li>
                <li>(120,0.25)</li> <!-- 120: 0.25 -->
            </points>
        </adaptDaysGrowthRateCurve>
        <pointsFactorFromAdaptDays>
            <points>
                <li>(-30,0.40)</li>    <!-- recover in 10 days -->
                <li>(0,0.80)</li>    <!-- start at 0.80 -->
                <li>(30,1.00)</li>    <!-- 0.5 years to 1.00 -->
                <li>(60,1.20)</li>    <!-- 0.5 years to 1.20 -->
                <li>(120,1.60)</li>    <!-- 2 years to 1.60 -->
                <li>(180,2.00)</li>    <!-- 4 years to 2.00 -->
            </points>
        </pointsFactorFromAdaptDays>
        <comps>
            <li Class="StorytellerCompProperties_DeepDrillInfestation">
                <baseMtbDaysPerDrill>20</baseMtbDaysPerDrill>
                <allowedTargetTags>
                    <li>Map_PlayerHome</li>
                    <li>Map_TempIncident</li>
                    <li>Map_Misc</li>
                    <li>Map_RaidBeacon</li>
                </allowedTargetTags>
            </li>
            <!-- Royalty begin -->
            <li Class="StorytellerCompProperties_SingleOnceFixed">
                <fireAfterDaysPassed>10</fireAfterDaysPassed>
                <incident>GiveQuest_Intro_Deserter</incident>
                <skipIfOnExtremeBiome>false</skipIfOnExtremeBiome>
                <allowedTargetTags>
                    <li>World</li>
                </allowedTargetTags>
            </li>
            <li Class="StorytellerCompProperties_FactionInteraction">
                <incident>CaravanArrivalTributeCollector</incident>
                <minDaysPassed>20</minDaysPassed>
                <baseIncidentsPerYear>2</baseIncidentsPerYear>
                <minSpacingDays>30</minSpacingDays>
                <minWealth>20000</minWealth>
                <allowedTargetTags>
                    <li>Map_PlayerHome</li>
                </allowedTargetTags>
            </li>
            <!-- Ideology begin -->
            <li Class="StorytellerCompProperties_WorkSite" MayRequire="Ludeon.RimWorld.Ideology">
                <incident>GiveQuest_WorkSite</incident>
            </li>
            <li Class="StorytellerCompProperties_OnOffCycle" MayRequire="Ludeon.RimWorld.Ideology">
                <incident>GiveQuest_Beggars</incident>
                <onDays>60</onDays>
                <offDays>0</offDays>
                <minSpacingDays>20</minSpacingDays>
                <numIncidentsRange>1</numIncidentsRange>
                <allowedTargetTags>
                    <li>Map_PlayerHome</li>
                </allowedTargetTags>
            </li>
            <li Class="StorytellerCompProperties_RandomEpicQuest" MayRequire="Ludeon.RimWorld.Ideology">
                <category>GiveQuest</category>
                <allowedTargetTags>
                    <li>World</li>
                </allowedTargetTags>
                <onDays>40</onDays>
                <offDays>0</offDays>
                <minDaysPassed>30</minDaysPassed>
                <numIncidentsRange>1</numIncidentsRange>
            </li>
            <li Class="StorytellerCompProperties_OnOffCycle" MayRequire="Ludeon.RimWorld.Ideology">
                <incident>GiveQuest_ReliquaryPilgrims</incident>
                <onDays>40</onDays>
                <offDays>0</offDays>
                <minSpacingDays>20</minSpacingDays>
                <numIncidentsRange>1</numIncidentsRange>
                <allowedTargetTags>
                    <li>Map_PlayerHome</li>
                </allowedTargetTags>
            </li>
            <!-- Biotech begin -->
            <li Class="StorytellerCompProperties_MechanitorComplexQuest" MayRequire="Ludeon.RimWorld.Biotech">
                <incident>GiveQuest_AncientComplex_Mechanitor</incident>
                <mtbDays>60</mtbDays>
                <existingMechanitorOrMechlinkMTBFactor>4</existingMechanitorOrMechlinkMTBFactor>
                <minSpacingDays>60</minSpacingDays>
                <allowedTargetTags>
                    <li>Map_PlayerHome</li>
                </allowedTargetTags>
                <blockedByQueuedOrActiveQuests>
                    <li>OpportunitySite_AncientComplex_Mechanitor</li>
                    <li>MechanitorShip</li>
                </blockedByQueuedOrActiveQuests>
            </li>
            <li Class="StorytellerCompProperties_DissolutionTriggered" MayRequire="Ludeon.RimWorld.Biotech">
                <incident>WastepackInfestation</incident>
                <thing>Wastepack</thing>
                <delayTicks>0</delayTicks>
                <allowedTargetTags>
                    <li>Map_PlayerHome</li>
                    <li>Map_Misc</li>
                </allowedTargetTags>
            </li>
            <li Class="StorytellerCompProperties_NoxiousHaze" MayRequire="Ludeon.RimWorld.Biotech">
                <lastFireMinMTBThreshold>0.5</lastFireMinMTBThreshold>
                <lastFireMaxMTBThreshold>2</lastFireMaxMTBThreshold>
                <allowedTargetTags>
                    <li>Map_PlayerHome</li>
                    <li>Map_Misc</li>
                </allowedTargetTags>
            </li>
            <!-- Anomaly begin -->
            <li Class="StorytellerCompProperties_MonolithMigration" MayRequire="Ludeon.RimWorld.Anomaly">
                <incident>MonolithMigration</incident>
                <mtbDays>4</mtbDays>
                <allowedTargetTags>
                    <li>Map_PlayerHome</li>
                </allowedTargetTags>
            </li>
        </comps>
    </StorytellerDef>

