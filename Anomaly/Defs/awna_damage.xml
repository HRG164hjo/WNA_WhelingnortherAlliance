<?xml version="1.0" encoding="utf-8" ?>
<Defs>
    <DamageDef ParentName="WNA_DamageBase">
        <defName>WNA_ShellPsychicDamage</defName>
        <label>psychic drunkenness</label>
        <workerClass>DamageWorker_Frostbite</workerClass>
        <!--<harmsHealth>false</harmsHealth>
        <makesBlood>false</makesBlood>-->
        <isExplosive>true</isExplosive>
        <deathMessage>{0} has died in an explosion.</deathMessage>
        <hediff>Crack</hediff>
        <hediffSolid>Crack</hediffSolid>
        <harmAllLayersUntilOutside>true</harmAllLayersUntilOutside>
        <explosionCellFleck>WNA_ShellPsychicFleck</explosionCellFleck>
        <soundExplosion>Explosion_Stun</soundExplosion>
        <impactSoundType>Blunt</impactSoundType>
        <buildingDamageFactor>15</buildingDamageFactor>
        <plantDamageFactor>15</plantDamageFactor>
        <corpseDamageFactor>15</corpseDamageFactor>
        <minDamageToFragment>15</minDamageToFragment>
        <defaultDamage>15</defaultDamage>
        <additionalHediffs>
            <li>
                <hediff>WNA_ShellPsychic</hediff>
                <severityPerDamageDealt>0.1</severityPerDamageDealt>
            </li>
        </additionalHediffs>
    </DamageDef>

</Defs>